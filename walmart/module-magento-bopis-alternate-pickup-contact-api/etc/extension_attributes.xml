<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Api/etc/extension_attributes.xsd">
    <extension_attributes for="Magento\Quote\Api\Data\CartInterface">
        <attribute code="pickup_contact" type="Magento\Quote\Api\Data\AddressInterface" />
    </extension_attributes>

    <extension_attributes for="Magento\Sales\Api\Data\OrderInterface">
        <attribute code="pickup_contact" type="\Magento\Sales\Api\Data\OrderAddressInterface" />
    </extension_attributes>

    <extension_attributes for="Magento\Checkout\Api\Data\ShippingInformationInterface">
        <attribute code="alt_pickup_contact" type="\Magento\Quote\Api\Data\AddressInterface" />
    </extension_attributes>
</config>
